PCM.ActionNetworkGateway = (function() {

  function fetchMarches(callback) {
    $.ajax('<%= ENV['SITE_URL'] %>/marches', {
      success: function(data) {
        $(data).each(function(idx, march) {
          callback(march);
        });
      }
    });
  }

  return {
    fetchMarches: fetchMarches
  };
})();
